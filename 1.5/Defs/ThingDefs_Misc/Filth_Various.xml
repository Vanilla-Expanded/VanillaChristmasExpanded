<?xml version="1.0" encoding="utf-8" ?>
<Defs>

 

  <ThingDef ParentName="BaseFilth">
    <defName>VCE_Filth_Water</defName>
    <label>water puddle</label>
    <statBases>
      <Beauty>-10</Beauty>
      <Cleanliness>-5</Cleanliness>
    </statBases>
    <graphicData>
      <texPath>Things/Filth/WaterPuddleA</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>2</drawSize>
    </graphicData>
    <filth>
      <disappearsInDays>3~5</disappearsInDays>
      <cleaningWorkToReduceThickness>30</cleaningWorkToReduceThickness>
      <canFilthAttach>true</canFilthAttach>
      <allowsFire>false</allowsFire>
      <cleaningSound>Interact_CleanFilth_Fluid</cleaningSound>
    </filth>
  </ThingDef>



</Defs>
